INSERT INTO `router` VALUES ('webform.reports_plugins.handlers','/admin/reports/webform-plugins/handlers','/admin/reports/webform-plugins/handlers','15','O:31:"Symfony\\Component\\Routing\\Route":9:{s:4:"path";s:39:"/admin/reports/webform-plugins/handlers";s:4:"host";s:0:"";s:8:"defaults";a:2:{s:11:"_controller";s:64:"\\Drupal\\webform\\Controller\\WebformPluginHandlerController::index";s:6:"_title";s:25:"Webform plugins: Handlers";}s:12:"requirements";a:1:{s:11:"_permission";s:18:"administer webform";}s:7:"options";a:4:{s:14:"compiler_class";s:33:"Drupal\\Core\\Routing\\RouteCompiler";s:4:"utf8";b:1;s:12:"_admin_route";b:1;s:14:"_access_checks";a:1:{i:0;s:23:"access_check.permission";}}s:7:"schemes";a:0:{}s:7:"methods";a:2:{i:0;s:3:"GET";i:1;s:4:"POST";}s:9:"condition";s:0:"";s:8:"compiled";O:33:"Drupal\\Core\\Routing\\CompiledRoute":11:{s:4:"vars";a:0:{}s:11:"path_prefix";s:0:"";s:10:"path_regex";s:47:"#^/admin/reports/webform\\-plugins/handlers$#sDu";s:11:"path_tokens";a:1:{i:0;a:2:{i:0;s:4:"text";i:1;s:39:"/admin/reports/webform-plugins/handlers";}}s:9:"path_vars";a:0:{}s:10:"host_regex";N;s:11:"host_tokens";a:0:{}s:9:"host_vars";a:0:{}s:3:"fit";i:15;s:14:"patternOutline";s:39:"/admin/reports/webform-plugins/handlers";s:8:"numParts";i:4;}}','4'),('webform.reports_plugins.variants','/admin/reports/webform-plugins/variants','/admin/reports/webform-plugins/variants','15','O:31:"Symfony\\Component\\Routing\\Route":9:{s:4:"path";s:39:"/admin/reports/webform-plugins/variants";s:4:"host";s:0:"";s:8:"defaults";a:2:{s:11:"_controller";s:64:"\\Drupal\\webform\\Controller\\WebformPluginVariantController::index";s:6:"_title";s:25:"Webform plugins: Variants";}s:12:"requirements";a:1:{s:11:"_permission";s:18:"administer webform";}s:7:"options";a:4:{s:14:"compiler_class";s:33:"Drupal\\Core\\Routing\\RouteCompiler";s:4:"utf8";b:1;s:12:"_admin_route";b:1;s:14:"_access_checks";a:1:{i:0;s:23:"access_check.permission";}}s:7:"schemes";a:0:{}s:7:"methods";a:2:{i:0;s:3:"GET";i:1;s:4:"