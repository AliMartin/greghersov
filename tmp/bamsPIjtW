INSERT INTO `router` VALUES ('webform_submission.multiple_delete_confirm','/admin/structure/webform/results/delete','/admin/structure/webform/results/delete','31','O:31:"Symfony\\Component\\Routing\\Route":9:{s:4:"path";s:39:"/admin/structure/webform/results/delete";s:4:"host";s:0:"";s:8:"defaults";a:2:{s:5:"_form";s:56:"\\Drupal\\webform\\Form\\WebformSubmissionDeleteMultipleForm";s:14:"entity_type_id";s:18:"webform_submission";}s:12:"requirements";a:1:{s:30:"_entity_delete_multiple_access";s:18:"webform_submission";}s:7:"options";a:4:{s:14:"compiler_class";s:33:"Drupal\\Core\\Routing\\RouteCompiler";s:4:"utf8";b:1;s:12:"_admin_route";b:1;s:14:"_access_checks";a:1:{i:0;s:35:"access_check.entity_delete_multiple";}}s:7:"schemes";a:0:{}s:7:"methods";a:2:{i:0;s:3:"GET";i:1;s:4:"POST";}s:9:"condition";s:0:"";s:8:"compiled";O:33:"Drupal\\Core\\Routing\\CompiledRoute":11:{s:4:"vars";a:0:{}s:11:"path_prefix";s:0:"";s:10:"path_regex";s:46:"#^/admin/structure/webform/results/delete$#sDu";s:11:"path_tokens";a:1:{i:0;a:2:{i:0;s:4:"text";i:1;s:39:"/admin/structure/webform/results/delete";}}s:9:"path_vars";a:0:{}s:10:"host_regex";N;s:11:"host_tokens";a:0:{}s:9:"host_vars";a:0:{}s:3:"fit";i:31;s:14:"patternOutline";s:39:"/admin/structure/webform/results/delete";s:8:"numParts";i:5;}}','5');
DROP TABLE IF EXISTS `search_dataset`;
CREATE TABLE `search_dataset` (   `sid` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Search item ID, e.g. node ID for nodes.',   `langcode` varchar(12) CHARACTER SET ascii NOT NULL DEFAULT '' COMMENT 'The "languages".langcode of the item variant.',   `type` varchar(64) CHARACTER SET ascii NOT NULL COMMENT 'Type of item, e.g. node.',   `data` longtext NOT NULL COMMENT 'List of space-separated words from the item.',   `reindex` int(10) unsigned NOT NULL DEFAULT 0 COMMENT 'Set to force node reindexing.',   PRIMARY KEY (`sid`,`langcode`,`type`) ) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COMMENT='Stores items that will be searched.';
INSERT INTO `search_dataset` VALUES ('1','en','node_search',' something rich and strange something rich and strange admin mon 9262022 2207  ','0'),('2','en','node_search',' 1 1 admin mon 9262022 2208 this is chapter one  ','0'),('3','en','node_search',' 2 great shapesphere 2 great shapesphere iframe iframefieldspotify3 frameborder borderwidth  your browser does not support iframes but you can visit admin mon 9262022 2209 james joyce gave shakespeare 1 numerous nicknames shapesphere is one i especially like it conjures up a shakespeare that i have discovered over the years a playwright who shapes a world inside a theatre that reflects refreshes and regenerates the real world outside to even begin to forge any individual connection meant doing battle with brand shakespeare 1 2  like all successful brands brand ws has mutated over time but it essentially insists that there is a correct way to speak stage and study the bard brand shakespeare also promotes the playwright as a national treasure a tourist attraction and a major asset to team gb s global soft power the branding starts at school where children are taught how shakespeare is good for them how they must bow down and worship him and all his works especially if they want to achieve the right grades productions of shakespeare are subject to a similar set of orthodoxies directors designers and actors should not transgress certain vague but absolute rules that audiences and critics know and revere brand shakespeare sells us the idea that shakespeare is universal his plays possessing eternal wisdom and beauty resistance to this brand was inevitable resulting in another brand shakespeare the dead white male dinosaur outdated and irrelevant and only fit for demystifying deconstructing and destruction by fearless academics deconstructed shakespeare derides old fashioned productions and demands staging that remixes shakespeare for a short attention span audience the result of this battle of the brands is stagnant shakespeare everyone becomes exhausted we don t really want to talk about king lear its long and depressing celebrity casting keeps us going so and so playing hamlet or cleopatra compels attention retrospective surveys of past performances star rankings box office income and now live international broadcasts we don t need to think about the characters hamlet is indecisive lear is mad and cleopatra is seductive we don t examine the plays for their aliveness beyond a few trite parallels a politician becomes shakespearean in their predicament we should be examining every new production for what hamlet lear or cleopatra have to say to us and the times we live in or indeed if they have anything to say otherwise we are just visiting museums worshipping at shrines or waving flags at theatre proms for my education the standard notion was that shakespeare s plays were meant to be read not seen this approach emphasised poetry imagery and moral themes above all else i found the plays far more magical and mysterious the way scenes followed one another juxtaposing atmospheres or how characters appeared and disappeared the vastness of the plays landscapes made recollecting the work in the tranquillity of the academic study seem painfully limited i needed to discover other ways of seeing shakespeare i found history very important reading about the social political and philosophical background to shakespeare s work had real value 2 the renaissance reformation was conducive to an extraordinary flowering of culture in england the elizabethan age was one of expansion in both world trade and imaginative thinking the country was becoming a new nation innovation and adventurousness thrived immersing oneself in the elizabethan age freed shakespeare from the victorian shakespeare i d grown up withthe victorian shakespeare reflected the ethos of the british empire britannia ruled the world because our morals our knowledge and our spiritual principles were the right ones shakespeare was thus an exemplar of the greatness of our island race his genius fixed and immutable rejecting victorian shakespeare and reclaiming renaissance shakespeare was essential to my growing understanding of the plays the other important perspective for me was seeing shakespeare as a practical working playwright actor who was a principle shareholder in a theatre company writing plays was his livelihood and his plays are illuminated by both practical and imaginative theatrical challenges for example shakespeare worked with a group of actors over a period of years and often wrote specific parts to expand both their range and his audiences imagination in the year 1599 shakespeare wrote three parts for the actor richard burbage you can trace a fascinating line from henry v through brutus to hamlet by looking at the acting challenges of each role not forgetting the missing link of rosalind i also found the story of how shakespeare s globe theatre was built inspiring a group of people stealing a whole wooden structure from one location and erecting it in another testified to a practical ingenuity and a total determination to build a special theatre for the people of london dame frances yates theatre of the world a crucial book for the founding artistic directors argues that the elizabethan public theatres architecture reflected some of the most advanced renaissance thinking about the nature of humanity the globe expressed the idea of the human being as a microcosm of the macrocosm each individual reflecting the cosmos a space open to the elements and emblazoned with the motto we are all players the globe aspired to be a theatre of the world a theatre that was a secular democratic alternative to the prime site for dispensing ideas and morality to the people the church this grand vision directly influenced the design of the exchange and mirrored my own convictions about the power and purpose of theatre my first effort as a director of shakespeare was a play i devised aged 23 called will the opening moment was a voiceover to the sounds of heartbeats and wind in the name of god amen i william shakespeare of stratford upon avon do make and ordain this my last will and testament we happily speculate about shakespeare s identity in the absence of biographical facts he is an artist whose work cannot be defined by endless details of his private life fittingly to find the truth about shakespeare his play s are the only real clues we have will was put together by collaging lines from all his plays to create a personal testament on the part of their creator thus it was will shakespeare creating a dramatic will for posterity and because he was revealing some difficult emotions it required an effort of will looking back i was fortunate in being able to take my own creative approach to the bard for my first attempt i was inspired by charles marowitz s cut up productions from my student days i liked the provocation of turning shakespeare texts inside out to produce completely different readings i also loved the rare international productions that came to london 3there were no captions in those days so the fact that their impact was frequently more exciting and immediate than english theatre productions taught me something about the inherent magic of the plays i was also drawn to the contemporary iconoclastic shakespearean cinema of derek jarman and aki karismaki rather than conventional cinematic adaptations their versions of the tempest and hamlet felt truer to the spirit of shakespeare they conjured up mysterious unsettling and beautiful sensations through the eclectic use of modern images and sounds all these influences inspired me in being arrogant enough to think i could use all shakespeare s 37 plays and a bit more to create an hour long piece of theatre i created a love play between a man and a woman that told the story of a doomed relationship from the first flush of youth to the miseries of a failed marriage underscored by the new york punk band television this was followed by a section called laughter directly inspired by trevor griffith s brilliant play comedians shakespeare and two actors explored the nature of comedy from the early gag oriented launce and his dog through to the satanic laughter of the porter and his knock knock routine the final act was about politics and shakespeare s oscillation between a wish for order and the need for chaos to gain insight when the character shakespeare had a breakdown i deliberately inserted a speech from edward bond s bingo 4 to demonstrate the depth of his disturbance will concluded with shakespeare reciting prospero s epilogue as he stared at all the titles of his plays which constituted the set design an artist acknowledging all the pain and pleasure he had imagined and finding some kind of self acceptance one moment in will reflected my own picture of shakespeare the man beggar what hast thou been will a serving man proud in heart amind swore as many oaths as i have spake words and broke them in the sweet face of heaven false of heart light of ear bloody of hand hog in sloth fox in stealth wolf in greediness dog in madness lion in prey whenever i read king lear this speech of edgar disguised as the mad poor tom always strikes me as strange in its length and detail is this speech a confession on the part of shakespeare i m a flawed contradictory messed up human being like everyone else devising and directing will freed me up in how i directed the subsequent plays i also found some tantalising lines that provide more food for thought as to who shakespeare was in romeo juliet we first see the franciscan friar laurence gathering flowers in the early morning for medicinal purposes this friar is a homeopath with a spiritual philosophy for naught so vile that on the earth doth live but to the earth some special good doth give nor aught so good but strain d from that fair use revolts from true birth stumbling on abuse the friar then extrapolates from within the infant rind of this weak flower poison hath residence and medicine power to the human heart two such opposed foes encamp them still in man as well as herbs grace and rude will perhaps we catch a glimpse here of how shakespeare saw both life and writing a balancing of elements with an understanding that all human qualities good and bad creative and destructive are part of who we are the enigma of his identity comes from this awareness of the complexity of the human mind for shakespeare no human being can be crudely labelled each one of us needs to try to find a way of balancing the opposed foes of grace and rude will after a year or so i realised that i d come to the ideal space for directing shakespeare the green forest of arden like auditorium the relationship of the inside of the theatre to the exterior hall the way the ceiling of the space formed a giant brain all reflected the original globe theatre watching shakespeare productions in the space the language sounded so vivid and immediate i revived my play will and that only confirmed that the exchange was the place for me to direct shakespeare i intended to progress in a rough chronological manner my first choice was a midsummer nights dream i would recommend this play to any director as their first attempt at shakespeare the play is accessible beautifully constructed and built to give pleasure to an audience however the play also directly challenges any director the title invites you to dream your own particular dream within the themes of the play there is no obvious way to visualise the fairy world of oberon puck and titania you have to use your imagination to its fullest the triple worlds of the fairies the lovers and the mechanicals are totally separate and i found i could only rehearse one of these worlds per day at the start of rehearsals to try and look at all three would lead to one s brain exploding i loved the challenge i felt i was being pushed to my limit as a director i didn t mind the feelings of hopelessness inadequacy and panic that i went through somehow i was always aware that if i kept pushing myself i would be able to create something out of this material actors enjoy being in this play but the director has to lead because any dream is subjective and possesses its own unique logic i was pleased with the overall spirit of my production aware of all my mistakes and hungry to do more i then directed the comedy of errors romeo and juliet in the space of a couple of years both early shakespeare one comic the other tragic i was discovering an overarching theme in the bard which i felt unlocked his work this theme is articulated most clearly in the bosun s agonised cry we split we split at the climax of the magic storm that sinks the king s ship in the tempest this sense of split worlds and divided selves is central to the friar s view of nature the flowers that contain both poison and medicine creative and destructive qualities in midsummer nights dream hermia talks of seeing things with parted eye when everything seems double i was seeing this split self split world pattern again and again in shakespeare one can speculate over the source of this artistic vision for me shakespeare must have been responding to the great currents of history the transition from a hierarchical but settled agrarian feudal order to a more individually based urban society of capitalist trade in commodities the conflict of catholicism versus protestantism where again a more individually based religious faith emerged from a more collective church based existence the plays are full of split families inter generational strife and human beings struggling between their material appetites and their more spiritual selves shakespeare continually recasts and refines this double vision until its consummation in the magical reflecting world of prospero s island the comedy of errors is generally classed as a comic trifle a play ripe for jazzing up with songs and gags galore prince edward the patron of the royal exchange theatre was paying a quick visit to manchester and attended a performance one thursday night with a normal audience as opposed to a specially invited group of local worthies my production started and the prince immediately laughed loudly i think he felt that if he gave a signal the audience could relax and participate in a comic evening no one reacted and i felt someone disappearing into their seat next to me as the evening continued the audience started to laugh and gradually the prince joined in genuine warm laughter ensued and a community for the play was formed the point of this story is that the comedy of errors has a mysterious opening scene that is emphatically not funny in fact the scene permeates the play with a seriousness and melancholic beauty that underlays the comic mayhem that ensues the play opens with the ruler of the city of ephesus about to sentence a merchant from syracuse to death these two cities are in a state of conflict over trade and as a result the two cities have decided ban each other from their territories 5 the duke asks the merchant egeon to briefly tell his story he proceeds to launch into a speech of over a hundred lines one of the longest expositions in the whole of shakespeare the language has an rolling incantatory rhythm reflecting the tale of an ordinary family being tossed and turned on the sea of life a tragedy that could happen to anyone of us astonishingly shakespeare introduces a subterranean comic feel egeon is asked to speak in brief but he does the exact opposite his story is one long series of mishaps reminiscent of a shaggy dog story that gets sadder and sadder until there is something horrifically comic in how this family lose each other at the end of the speech the condemned man doesn t care if he dies and the ruler is full of compassion for this alien but is stuck in his role as state enforcer everything is off kilter almost absurdist the picture of a split world is overwhelming the number two dominates the story two sets of twins and two ships to the rescue egeon s tragic mishaps result in a veritable comedy of errors but the play s furious fun is framed by a planned execution at sunset the comedy of errors is a day in the life of the human race divided and desperate but also irredeemably comic in shakespearean comedy a divided world unites through the actions of its characters however chaotically achieved in shakespearean tragedy the outcome is more brutal romeo juliet takes place in a divided city dominated by two opposing families yet the most destructive split is between the generations the play ends in a family vault four young people lie dead inside romeo juliet paris tybalt mercutio has died earlier benvolio disappears thus we witness an entire generation being wiped out their parents and rulers being left to pick up the pieces the two warring patriarchs montague and capulet resolve to erect two pure gold statues of the lovers to stand in the city centre as poor sacrifices of our enmity when i was thinking about directing the play i wandered around various cities looking at public statues i saw statesmen soldiers sportsmen and writers but no young lovers anywhere statues record what is seen as valuable to our society which is why shakespeare ends the play as he does he wanted romeo and juliet to stand as a monument celebrating the love that transgresses divided worlds romeo and juliet s love subverts an ancient feud they are outsiders who find a love that threatens the status quo to be sure romeo and juliet are awkward adolescents but they break boundaries and they are prepared to kill themselves rather than live in a world that denies their love 6 the absence of lovers statues in city centres testifies to the fact that shakespeare s story of woe is still happening around the world every time romeo juliet is performed it should serve as a warning i have not come to the royal exchange theatre to see a man in dirty trainers speak shakespeare s verse a furious person shouted outside the theatre having noisily walked out of the opening scene of the comedy of errors at a q a for romeo and juliet someone said that they loved the modernity of my production except for one thing why couldn t the characters look like the zeffirelli film i pointed out that the film was twenty five years old and indeed zeffirelli s original stage production was hugely acclaimed for its modernity in evoking italian street machismo the person replied yes but they looked so beautiful in those colours and tights and all that red velvet two differing reactions but with an underlying feeling that as one dresses the right way for dinner so there is an appropriate etiquette for how shakespeare should look the sense i had of the comedy of errors and romeo and juliet led to productions set in the now i use this phrase instead of words like modern or contemporary because it helps define my approach to staging shakespeare in the now means thinking deeply about the play it s characters imagery structure and themes this entails researching the historical social and cultural context of the play equally one must think deeply about how the play connects to our current times this entails keeping your eyes open to everything going on around you i would compile files of cuttings from newspapers magazines photographs that reflected the life of the play my production would represent a synthesis of this thinking staging shakespeare in the now is a challenging process to sustain an imaginative vision throughout a five act play is hard shorthand modern ideas such as mobile phones cctv computers are as empty as decorative period objects unless properly integrated into the fabric of the play to return to that man in dirty trainers egeon is a homeless wanderer a man who has been travelling the world for some years in an increasingly futile search for his family just think about migrants in our globalised universe so many anonymous people in cities with epic family sagas of hope and despair i wanted egeon to look like those haunted figures we come across everyday whom we mostly ignore but have a story to tell hence the dirty trainers this man had been doing a lot of walking staging the comedy of errors i wanted to create a world that reflected the split between the vulnerable insecure syracusans and the materialistically driven ephesians the action of the play takes place in various locations around a city during one day manchester at that time was starting a process of redevelopment branding itself as the 24 hour city this was part of the overall homogenising of urban cities reflecting the increasing globalisation of the economy now we have brand lifestyles available in every major city in the world we can drink the same coffee and buy the same clothes everywhere ephesus became a modern city a ruler suspended on a steel throne over a destitute man a wife cooking a gorgeous magazine dinner for her errant husband a park full of lonely people metro buses rubbish dumps a cage dancing club a robocop dr pinch as a new age track suited guru and a myriad of modern city dwellers going about their lives reflecting the play s comic nature the stage floor looked like an industrial circus ring that meant each scene was set up like a variety turn sometimes the visual language was overtly vaudevillian a revolving door in the round that made the actors follow its direction a slapstick fight with rubbish bags or plunging through a paper hoop held by nuns to escape into a priory public plazas back alleys ideal homes the comedy of errors packs an enormous amount of urban lifestyles into one day in the life of a city it s frenetic activity reflects our split selves that shakespeare is exploring i wanted to find a unifying element for this world in one of my favourite films robert altman s reimagining of raymond chandlers classic the long goodbye the fatalistic title song pops up everywhere in different musical guises for the comedy of errors i chose ive got you under my skin it seemed appropriate for the split identities running through the play the tune was heard throughout the play in different versions a lonely saxophonist in the street a funked up cage dancing version a nuns chant in latin backwards for dr pinch s followers even ice cream van chimes for the curtain call we finally went for the full frank sinatra the multiple versions of the tune emphasised that the characters in the play however seemingly disparate were all singing the same song at the plays climax the action moves to egeon s place of execution by this time most of the characters are dazed and confused the absent mother returns the family is reunited and all confusions ironed out the split world is no more and everyone has learnt about their shadow sides during the comic confusion of one day audiences and readers frequently dismiss these act5 reconciliation scenes as shakespeare lazily tying up his plot with a vague happy ending actually if one truly understands each individual character these scenes are full of wonder actors need to identify and register every moment in such scenes they will discover an enormous amount of material to play i can well imagine in many parts of the world that the ending of the comedy of errors with a family split by natural and manmade disasters finally finding each other would be extremely moving romeo juliet is also set in the city my thinking about the doomed youth generation in the play coincided with the madchester experience of the early 1990s one day i was in the whigs department and overheard a conversation about weekend plans we re off to a field somewhere sounds great what do you mean i asked i was told there would be a sound system and twenty thousand people dancing the night away i felt like dylan s mr jones something was going on and i didn t know what it was the whole look and attitude of the youth was in a particularly vivid moment the young cast helped myself and the designer david short in how the characters of our modern verona might look one critic wrote this was a romeo juliet for the happy mondays generation i took that as a compliment shakespeare very rarely indicates specific objects onstage when he does we should pay attention romeo juliet moves inexorably towards its climax in an ancient family burial vault the play is full of quicksilver energy even though it s a tragedy the action moves with the energy of its young protagonists but ultimately the static world of the dead is waiting to claim the living in palermo there is a famous capuchin crypt where whole generations were embalmed and preserved for centuries we used this reference for the capulet monument suddenly the hitherto largely open space of the exchange was filled with ornate tombs with fully costumed skeletons from different centuries romeo and juliet would come and join them this sudden visual injection of death s dominion honoured shakespeare s rare stage direction for the play s climax and in the round the emotional impact of these decorated skeletons was doubled for an audience strangely my major staging challenge was the balcony scene the royal exchange theatre has two balcony levels surrounding the stage what could be simpler actually the theatre is constructed in such a way that the balconies overhang the stalls which meant that a significant amount of people would not see juliet we tried to solve this but the idea of building a balcony or wheeling one on seemed feeble in the end i decided we should simply discard the balcony and start afresh this resulted in what we called the barbecue scene in a moonlit garden with the wind rustling through the trees juliet came down to meet romeo around a large barbecue used for her father s party being on the same level bought up all sorts of questions notably would they touch or even make love we decided in rehearsals that they had too much respect for what they were feeling about each other their romance gained a distinctive sensuality by actively eschewing physical contact i was desperately trying to cast romeo i had a strong idea who i was looking for but was getting nowhere fast a succession of vaguely charming actors came in their readings were sentimental cliches generalised ideas about adolescent love with no belief in the individuality and outsider nature of the role after a whole week of seeing people friday afternoon loomed and time was running out casting director sophie marshall informed me that the next person on the list was rada s best actor from the year before who had gone straight into a west end play you ll probably hate him she said in the long suffering tone of a casting director frustrated by a director not liking anyone the door opened and a young curly haired gypsy looking welsh guy walked in we started talking about the latest david lynch film and the doors he told me about a welsh band the manic street preachers who i had never heard of he talked about growing up in recession hit port talbot he knew of fellow teenagers who saw no future and had simply driven motor cars into walls suicide was a real choice there he saw the play in similar terms i suggested he might read the mantua scene where romeo is a small town boy lost in the big city the reading was fresh and real after he left i was gurgling like zero mostel that s our romeo i d just met michael sheen michael s focus was always on telling the story not just the plot but also the emotional linguistic and moral themes of a play understanding one s own role was important but also understanding the whole story and all the other characters actors of previous generations would often work in their own bubble and listen politely until it was their turn again michael was continuously alive in his work reacting to every moment in a scene and thus creating life for the other characters rehearsals were exciting and unexpected michael would come in with a mass of ideas that he would play with he possessed a formidable technique that enabled him to explore without strain or stress he approached romeo as a modern young man facing a difficult world yet he also possessed a total awareness of the vast beauty of shakespeare s tragic vision michael was an interesting masculine actor being a keen footballer he had the drive determination and physicality of an athlete but he also embraced being vulnerable exposed and lyrical this was a romeo who could convincingly woo juliet and kill tybalt a romeo who could willingly commit suicide for love a contemporary film we both loved and referenced was les amants de pont neuf a wild unashamedly romantic piece about young lovers i felt michael s generation of actors were far more capable of playing intimate scenes the rehearsal rooms i first encountered were masculine centred and often toxic female actors had to be very resourceful in establishing a voice in the room speaking up was frequently met with muttering about feminism or accusations of being nuns an infamous remark by an eminent director about the new generation of articulate actors michael s generation was different i think improved education was a factor there was more respect and support for everyone to be heard growing up in teenage wastelands with the feeling of being isolated atoms in an unforgiving universe maybe also helped female and male actors come together the whole period vs modern staging debate has changed in recent years our culture has a compulsive need to return to past worlds in fictional histories fantasy sagas or superhero franchises these often barbaric landscapes reflect our contemporary fear of a post climate change world with a lack of resources a violent hobbesian universe where all our modern psychoses can be explored and heroes found to right the world s wrongs game of thrones shakespeare has arrived actors love being dressed up in chain mail and wielding big broadswords having previously hated the idea of being dressed in tights corsets and codpieces we travel back to the past with no wish for historical accuracy we unashamedly enjoy characters and story lines steeped in contemporary attitudes shakespeare is the upstart crow a period dressed sit com stalwart for myself an in the now approach is still the most exciting way to go it requires a kind of sustained creative thinking that the plays demand more importantly shakespeare s language spoken in contemporary clothes actively enhances our modern selves through its eloquence and beauty the past can strengthen the present as it is still alive in us not much is really new in our world life is more a perpetual dance my planned chronological canter through the bard s plays was now rudely interrupted casper wrede informed me that i would direct king lear with sir tom courtenay as my next shakespeare production casper always had plans and there was no question of my saying no tom courtenay was central to the royal exchange theatre from the beginning a working class boy from hull he was in the vanguard of a new wave of uk actors at the end of the 1950s he then went on to become an international film star but tom gave it all up and ran away to join the 69 theatre company in manchester the precursor to the royal exchange i had grown up watching his performances in the loneliness of the long distance runner and billy liar and now i was to work with this iconic actor tom affects a diffident almost absent minded manner of talking but underneath he s as sharp as a tack he listened very carefully to all my wild and whirling words about the play while working away with a single minded ferocity the idea that king lear is the summit of an actor s career was very real to him even though he was part of the new wave tom s relationship to the great world war 2 generation of uk actors was palpable he would tell me stories of olivier scofield and gielgud and talk about acting on stage with celia johnson who he saw as a genius he would recount what advice alec guinness had given him about playing lear apart from being fascinated i could see that there was a link between the generations tom was proud to be part of a generation that changed the face of uk acting he saw it as part of the opportunities that the post world war 2 labour government had given working class people but he was always respectful of his ancestors in his book dear tom letters home he characteristically refers to footballer billy bremner an uneducated kid from the backstreets of glasgow who was more intelligent on the pitch than some of his a level teammates tom then talks revealingly about himself we all need our element his brains were in his feet my brains weren t in my feet but neither were they entirely in my head they had slipped a bit towards my heart and my stomach and had spread close to my skin tom literally couldn t act something he didn t emotionally respond too a character had to have humanity and indeed beauty for him to accept a part this has led to him having a catholic career as he has turned down many big opportunities because he felt nothing for the role tom recently said to me that for him acting was a way of facing up to life the complete opposite of the usual idea that acting is an escape from life into fantasy for tom a role is a search for truth acting has a spiritual purpose that must be respected this quality of total emotional immersion was tom s uniqueness as lear fusing his humanity musical sense and technical skill he would literally expand with the massive feelings lear experiences his whole body and voice would be filled with an almost unearthly force those peaks were special to behold towards the end of my productions i would always watch a performance from outside the theatre in the great hall i liked to see all the work that went into what happened inside i would also listen to the actors through the glass when lear was grieving over cordelia tom s agonised voice was at an unearthly pitch that literally sent tremors into the theatre s foundations he was not shouting but was operating on that mysterious frequency of truly great acting king lear is shakespeare s most intimidating play to direct he deliberately changed the happy ending of his source material what was his darker purpose in writing a play that showed a country riven from top to bottom king lear is a jacobean play james 1 s emphasis on the divine right of kings his battles with parliament the sleaze and corruption of his court as well as his preoccupation with demonology created a fractured nation that culminated in a civil war some twenty years later shakespeare prefigured the violent nihilism of the jacobean drama perhaps he was undergoing some severe personal crisis we don t know we all have moments where we look at the endless destructiveness in the world and react with anger and despair especially as we get older and apparently no wiser i ve never directed a play that makes you feel so inadequate every idea you have feels insufficient the play looms in front of you huge and immovable after a while i realised that directing the play was similar to the world of this pre christian england tough unforgiving relentless like climbing a mountain step by step by step you have to prepare meticulously stay determined and realise that there is no right way to do king lear just your own version with your creative family in a particular space at a particular time i did my own edit of the text cutting around 600 lines and arranging an order of scenes out of the two versions available after doing this i found that the scenes were surprisingly comprehensible for an audience lear is not the most difficult shakespeare play to understand by a long way they were just all so massive emotionally and there were so many of them one after another i had only five weeks of rehearsal one day in the middle of week three i had worked all morning and had a production meeting in my lunch break with all the technical departments during the meeting i contemplated the rest of the day s schedule i had set myself i was due to rehearse all the storm scenes in the afternoon and continue through to gloucester s blinding in the evening i realised i had to get all the scenes into a dynamic shape as i still had acts 45 to rehearse and wouldn t be able to return in detail to these scenes until the final week before opening i felt bad i wanted to go home and just curl up in bed tom strode into the room and announced to everyone i have no idea what to do in these scenes but i m sure you will tell me exactly what to do greg i felt worse luckily help was at hand david tennant who was playing edgar was chafing to get into rehearsals edgar having a marginal presence before the heath it was also david s first scene with tom courtenay david s poor tom exploded into the room the scenes started to pop movements came naturally as tom and the other actors simply reacted to what david was doing highly inventive actors like david come to rehearsals with many ideas and an infectious energy i sailed through the rest of the day my crisis over and determination restored the standard question when i was at school was king lear optimistic or pessimistic teachers told us to assemble examples from the play pessimistic gloucester s as flies to wanton boys are we to the gods they kill us for their sport optimistic edgar s the gods are just and of our pleasant vices make instruments to scourge us depressed albany s humanity must perforce prey on itself like monsters of the deep we were supposed to choose one of these outlooks as the key to the play the problem is that all these feelings depend on the dramatic context gloucester undergoes many emotions before his death his despairing cry at an absurdist universe is just one of his outbursts edgar s seemingly positive vision continues with him saying his father s vices in begetting his bastard brother edmund cost him his eyes which is not a very uplifting example of divine justice albany is an articulate intelligent man who is permanently out of his depth his lines are memorable but also reflect his helplessness and inadequacy as a leader king lear defies multiple choice answers i think intergenerational strife lies at the heart of the play unlike romeo and juliet the younger generation are given power goneril regan cordelia edmund and edgar embody different forces that try to shape the future of society shakespeare characteristically doubled his families from his sources in order to create a comprehensive picture of how sibling dynamics in two motherless families with absolutist fathers creates chaos for all the outright hostility between generations is shocking the blinding of gloucester is horrific because the breakdown of communication between young and old explodes into sickening violence in an utterly convincing fashion this fissure affects the whole play even language itself is threatened king lear is a tower of babel symbolised by the fool who s acerbic surreal comments culminate in an enigmatic nonsense prophecy then shall the realm of albion come to great confusion 6a the prime site of this great confusion is lear himself he announces his darker purpose at the start of the play lear has a plan and tis our fast intent to shake all cares and business from our age conferring them on younger strengths while we unburden d crawl toward death shakespeare has another plan lear is made to undergo a painful odyssey before he dies his crawl towards death is full of rage and suffering but also insight and self knowledge his mind may crack but his massive ego drives him on by the time he draws his last breath the leader of the nation has expressed a whole mass of emotions revealing a man of multiple selves and possibilities the complete opposite of the monolithic mentality of a monarch at school and for a long time after lear s madness was an awkward subject to be skirted around with our new understanding of the state of dementia lear s madness has become comprehensible i experienced living through my father s progressive fragmentation in recent years once he woke up with my sister sitting with him i watched the emotion in his face shakespeare s lines came to mind as the only way to describe what i saw you do me wrong to take me out o th grave thou art a soul in bliss but i am bound upon a wheel of fire that my own tears do scald like molten lead lear is a portrayal of the ultimate splitting that awaits us all in old age to return to the question king lear optimistic or pessimistic i would answer realistic the play unflinchingly charts the disintegration of the world that we know the play also abounds in showing the resilience in human beings there are moments of compassion tenderness and courage throughout for every captain who accepts edmund s order to kill lear and cordelia there is the servant of gloucester who attempts to prevent his blinding lear cordelia the fool kent gloucester edgar all resist the nihilism of their times even the destructive characters have their reasons i wanted to unleash all these energies in the play without fear or simplistic moral posturing the exchange also offered a special way for bringing this vast tragedy to a harmonious conclusion as king lear progresses the action moves to dover the characters congregate for a final conflict at the very edge of the country the exchange is the original sense around sound space i surrounded one side of the theatre with the sound of the rolling sea throughout the final act at the end of the play everyone living carried the dead off kent carried lear edgar carried edmund the dead goneril and regan also the living bore the dead who were still with them the sound of the sea grew and grew in volume finally breaking over the entire theatre casting the space into darkness we all now know what we know and we go on with that knowledge this might be the end of the world as we know it but it is also the beginning of something else humanity endures there are so many things you fail at as a director of this play but i was happy with the spirit of this ending king lear is simply for real whether the play is optimistic or pessimistic is beside the point life in all its cruelty and destructiveness needs to be confronted by art shakespeare portrays humanity in its worst light but also at its best today in the western world king lear is often seen as being too bleak a word that is frequently used to denote a work that is too depressing a downer with no redemption a much abused word the implication being that the play is not worth dealing with we don t want to hear about all this suffering and violence i say western world because i m sure in other parts of the planet this play would be seen as an urgent vivid unforgettable story that would speak directly to its audiences king lear is not the problem we are the problem if we can t confront enjoy and be inspired by shakespeare s most profound play about humanity s divided state what in ill thoughts again men must endure their going hence even as their coming hither ripeness is all come on i had been taking a break from shakespeare for a few years after lear when the exchange s 30th anniversary season loomed and i found myself irresistibly drawn to the tempest the tempest is partly about how to create magic in the theatre it is a play that asks what is special about the space you are directing the play in as a director your imagination is stretched to the limit you are challenged at every moment in every conceivable way yes shakespeare casts the director as a prospero how potent is your magic i had known pete postlethwaite for a long time from the days when he was primarily a theatre actor before his film career took off pete came out of the late 1960s a time of experimentation and risk taking he was what i called a rock and roll actor i wanted to be free to create a production that had no inhibitions about where it had to go i needed a prospero who possessed wisdom and humanity but also a wild divided soul an actor prepared to go on a gruelling emotional journey and who wasn t afraid of playing a magician pete loved going out on the edge he also loved shakespeare and was a technically assured classical actor he was an actor who could make this most mysterious of plays come alive for a modern audience preparing the tempest was both exhilarating and frightening a key discovery i made was when prospero is talking with his daughter miranda he says his life now depends on a most auspicious star which he must follow if he is to have any future this gives the play a compressed energy and urgency that is easy to miss 7 prospero s project must advance in every second of stage time he is a man attempting to reorder his internal and external world to create a new society with the clock ticking the tempest is significantly shakespeare s only original story perhaps he was emulating prospero at the end of his theatrical life i suggested to pete that he should start learning prospero s lines a few months before rehearsals with major shakespearean roles this is advisable given the relatively short rehearsal periods we normally have something tom taught me with lear one day i got a phone call from pete he said he simply couldn t learn the lines he sounded despondent in fact as he went on talking somewhat desperate he later told me that he was about to withdraw from playing the part i realised that a conventional darling you re going to marvellous approach wouldn t work i thought feverishly and then decided to tell him what i was going through i said i had days where i would just stare at the text and nothing would come to me no ideas no feelings no images just a sense of total inadequacy i could literally find no meaning in the play but if i approached the text in a state of complete creative concentration armed with all the ideas and convictions i had the words would come alive every scene became vibrant with possibilities full of twists and turns and moment to moment revelations pete and i had been discussing prospero for some time and i suggested he looked at the lines in character he phoned the next day and said the lines were flying in the tempest begins by hurtling the audience into a storm full of sound and fury the nautical accuracy in the description of a ship capsizing is striking shakespeare wants to dissolve the barrier of staring at a play as a mere spectacle he wants an audience immersed in a spectacular occurrence i was interested in seeing this world before the storm hits at the time of the production there was a glut of articles about super yachts as an essential accoutrement of the super rich i had the courtiers sitting on deck sipping cocktails served by stephano and trinculo as they read magazines and relaxed after a successful foreign jolly i wanted to set up a picture of a global elite floating along without a care in the world music piped out mantovani s version of born free everything moved in a leisurely fashion as audiences took their seats the storm started with a sound a series of judders that jolted the characters out of their repose the sound then built in intensity surrounding the actors and audience alike waves crashed against the auditorium earthquake tremors shook the theatre s foundations wild music of the spheres assaulted the ears and the live screams of the crew could be heard echoing outside in the hall the actors slipped and slid around the stage the bosun manned a steel ladder t a polish bosun this being the time of the first brexit outburst at polish plumbers gradually the sound engulfed the whole theatre the actors disappeared and the ladder jackknifed up into the heavens with the bosun hanging on for dear life screaming we split we split the doors of the exchange opened and a shadowy man entered a few hand signs with his staff and everything subsided the sounds faded the lights changed revealing an elderly man and a young girl in an empty cavelike space a formal masque forms a central part of the tempest 8 the goddesses iris ceres and juno plus assorted nymphs and reapers appear to ferdinand and miranda the spectacle involves spoken words song and dance for some years i had been thinking about directing the play but i was always stymied by this scene shakespeare wants to create a significant moment but i couldn t connect with the spirits in prospero s masque i was haunted by one of my favourite shakespeare films derek jarman s the tempest in jarman s film the masque is a party scene where elizabeth welsh sings stormy weather backed by a chorus of sailors i found this unexpected playful and moving a distinctive masque for a distinctive tempest i wanted to do the same i started by putting a line through the whole scene and proceeded from there what is this masque for discussing this with pete we concluded that prospero wants to accomplish something beautiful and generous for his beloved daughter which given his magical powers he is capable of doing miranda is betrothed to ferdinand so perhaps any words we used would be about love the tempest was part of the 30 th anniversary season of the royal exchange theatre and as i ve already said any production of this play should be a celebration of the special features of the theatre where it is being performed the exchange had presented 21 of shakespeare s plays by this time i set out to find a line about love from each play i returned to my roots and plundered will combined together these disparate lines could form an encyclopaedic poem about the nature of love not just the joys of first love but also the more difficult feelings that come later an apt parental lesson for miranda and ferdinand but who should say these lines it came to me that the spirits of the island in a sense could be the actors who had spoken these words in the space why not find 21 exchange actors and ask them to record individual lines sound designer pete rice travelled the country and even recorded stuff from new york some actors like tom courtney michael sheen and janet mcteer spoke their actual lines others were actors who had played the part elsewhere and some were simply actors who i felt meant a lot to the history of the exchange nicknamed tempest aid it was a pretty impressive lineup our masque did not end there we continued with the rest of the cast reciting in unison the sonnet love is not love which alters when it alteration finds from the top level of the theatre shakespeare s sonnet of enduring love contains the apposite line that looks on tempests and is never shaken then we heard some actual original lines from the tempest masque in a sung recording by a mass choir the huddersfield singers this was followed by ariel singing a country western you nymphs called naiads of the wind ring brooks miranda and ferdinand slow danced the theatre was lit by coloured light through the surrounding glass and the other actors danced with the theatre ushers in the outside hall a fusion of all the elements of the exchange and the text in a moment of joy my long game being when prospero suddenly dismisses the event what we had all experienced gave our revels now are ended an intense context pete found a wonderful bittersweet quality in the speech partly because our masque had been genuinely substantial and special strangely our central staging challenge was one single object prospero s staff the staff unites the visual and interpretative concepts of any production of the tempest the staff asks the question what kind of magician is prospero very quickly horrible images of old testament prophets with staffs or star wars light sabres started to loom exploring what might have been in prospero s magic books i found a literature that was comprehensive and visually ravishing charts and maps of human behaviour that are astonishing in their detail occult thought has a long and eminent history magicians would be experts on humanity in all its elements astrological psychological and medical the dubious part of the occult was the active conjuring of spirits or the attempt to manufacture real gold however the theatre was a safe space for shakespeare to portray a magician a place of make believe where magic tricks can happen discussing all this with pete postlethwaite in the months before rehearsal often in his beautiful home in the cotswolds the kind of staff we needed started to emerge the staff should be an extension of pete s use of his body and mind to perform the powers that prospero possesses whether affecting other characters through a hand movement or conjuring up sounds lights and objects in a second the prospero actor should be able to initiate that moment through his staff and not be dominated by it in theory we were progressing but in practice what were we looking for designer ashley martin davis understood and agreed with the concept but how should we realise this most precious prop a few weeks went by then pete phoned me he said he d found a piece of wood in his garden that he thought was perfect he was going to work on it and would show us at the start of rehearsals the suspense was considerable pete came in with a piece of wood about three feet long and a few inches thick long and thin starting straight then curving slightly into another straight section pete had varnished and smoothed the surface himself he added a few details small personal family possessions and a friend had engraved a pentagram on one end he also had a small noose at the other end so the staff could hang off his wrist casually ashley added a tiny glitter effect at one end and our diy staff was complete pete could play with this staff like an orchestral conductor the staff thus became an extension of prospero s mind at any given moment however how on earth could we break this staff the answer was why should we why should prospero literally break his staff he has to abandon his magic powers that is the point the solution presented itself when prospero releases ariel he should hand him his staff ariel is the magic spirit of the island prospero has to return to the real world the releasing of ariel is simultaneously prospero s renunciation of his magic powers this moment of farewell became a wonderfully charged moment between pete and steven robertson ariel every night the end of a special collaboration in every way as soon as he handed the staff over prospero collapsed to the floor the lights changed the spell is over now prospero was in the exchange with us standing on a landscape of sand his epilogue was a recovery a farewell and the final step in his spiritual journey he stepped off the sand walked out of the theatre we could hear his footsteps echoing in the outside hall as the lights dimmed prospero has to return to the real world the actor has to relinquish the role the audience has to leave the exchange and go back to their own lives we are all one at the conclusion of shakespeare s magical creation i thought i was done with the bard at the exchange after the tempest i couldn t see what else i might have to say about shakespeare however meeting the actor cush jumbo changed my attitude while pygmalion was running over tea at the midland hotel i asked cush if she would play rosalind in as you like it i wanted to work again with this young exciting twenty first century performer who could rejuvenate classical roles for our times as you like it was a play i knew well from school onwards i d always loved the play in a vague way but had never thought about directing it but my sense was that it had a special magic and after the tempest magic and shakespeare was on my mind preparing the play i was struck by how much the language operated on several levels and according to the footnotes the third level was more often than not an erotic one this is reflected in the play s title the word it in elizabethan times was a euphemism for making love if you stress one word in the title you will get a different idea of lovemaking as or you or like or it you can double up the stresses and each combination will give you a new sensation about the play it seemed to me that shakespeare wanted to write a play that made love to an audience in a myriad of ways there are a million ways of making love solo duets trios and groups as you like it contains all these permutations any production needs to explore the play in a spirit of freewheeling desire and be eager to conjure up as many experiences as the title literally contains as you like it was the first play that shakespeare wrote for the new globe theatre and i think he wanted to give his acting company opportunities to shine apart from the four love relationships that are gloriously varied there is jacques and his seven ages of man touchstone s virtuoso turn on courtly quarrelling oliver s magic realist account of his brother saving his life adam s story of being thrown out of his job as well as songs galore these are all actor friendly party pieces we know shakespeare wrote for a company of 1215 actors all the actors would have inhabited the edgy urban court of duke frederick and then travelled to the rural magical forest of arden there are obvious doubling possibilities for individual actors the two dukes le beau jacques adam corin but the whole ensemble would have undergone the experience of playing two contrasting worlds as you like it is about an existing unsatisfactory society being transformed by living in a forest shakespeare wants the actors to mirror that process in their creative contribution to the play as you like it offers an opportunity to create a very special acting ensemble unlocking that potential is crucial for any revival in the middle of this ensemble shakespeare creates one of his most charming and fascinating characters rosalind is passionate poetic witty warm curious brave enthusiastic full of merriment as well as a determination to change things she is also impulsive temperamental and vulnerable in other words full of contradictions her motto fittingly being the wiser the waywarder there is no one way to play rosalind the actor needs to be supercharged with verbal wit emotional intelligence great physicality and a fearless adventurousness but rosalind s question in her opening scene what think you of falling in love can only be answered in my opinion by an actor fully expressing their own unique individuality in the role creating ganymede is the main challenge for the actor playing rosalind how to be a man being awkward and making mistakes is part of the play s comedy but ganymede is a far more brilliant creation than just getting easy laughs ganymede has to be a charismatic pal for orlando otherwise the wooing game would never take place orlando has to be completely caught up in ganymede s world also rosalind is exploring what it means to be fathom deep in love and becoming ganymede affects her personality at the play s end rosalind is a different person she has released herself through ganymede the future leader of society has been both female and male in front of our eyes cush started exploring ganymede by investigating physicality and being a bloke s best mate after a couple of weeks she asked me how i thought we were doing i said i felt the work was fine but was looking for that leap that would reflect why she cush was playing the part before a monday rehearsal of the scene where ganymede first appears i received a text i have found something over the weekend will try it out if you don t like it i will go back and try again we started the scene and came to the moment where rosalind decides to speak with him orlando like a saucy lackey cush was on the edge of the space crouched down with celia there was a longish pause then she jumped a big one into the space she landed planted her feet and transformed herself into this strutting razor tongued street kid engulfing orlando and all of us watching with a vocal delivery infused with modern rhythms at the end of the scene cush came over and quietly asked if i liked her ideas hell yes i answered some years later cush told me she had watched a clip of dizzee rascal over that weekend this made her think about all the grime mcs she had grown up watching she thought their wit rhyming and drive might fit ganymede for cush this was an important revelation her life experience could intersect with shakespeare the final challenge for the rosalind actor is the epilogue if the audience has fallen in love with the performer an actor can bask in the glory and get all the laughs but i felt there was far more to this moment i d already been surprised by the similarities between the forest of arden and prospero s island during the epilogue rosalind says my way is to conjure you conjure is a loaded word it means employing some kind of magic rosalind has transformed herself and her union with orlando transforms society the person who addresses us is leaving the forest to become the co leader of a new world i wanted there to be an extra dimension to rosalind s final line bid me farewell similar to prospero s set me free we are saying farewell to this extraordinary woman we are also saying farewell to our communal experience of the play and returning to the real world i felt there should be something magically ceremonious in this farewell i had no idea how to achieve this i spoke with cush about my idea she nodded and said give me a couple of weeks and i will show you somethinglater on in rehearsals cush said she had something she performed the epilogue on the word farewell she made a deep wide slow motion curtsey then she swept up an invisible something from the floor and turning 360 degrees offered it to the audience with her hand she then walked off quietly absolutely beautiful unfortunately the auteur director had another ending in mind the production design was shaped around the experimental idea of a bunch of sound speakers of various shapes and sizes that moved in and out of the space with the sounds of the forest with a vast abstract painted floor this was a a crouching actors flying speakers concept so to speak after rosalind s speech i intended to end the show with a final stunning sculpted tableau using all the sound speakers and accompanied by the live electric guitar of our troubadour character informed of my intentions cush looked at me with a glint in her eye and said so i have live music to accompany the epilogue and the speakers have the final moment she proceeded to herd james the troubadour into another room and start working cut to the technical rehearsal i had spent a good few hours the night before plotting the final shape of speakers with pedro the operator of the computerised flying system the technical was long and arduous and we were well into the final evening session we finished the final ensemble moment a rendition of the transposed song it was a lover and his lass with plenty of arcadian fire cush gave the epilogue with maximum intensity and floated out of the theatre james and his guitar likewise faded away into the darkness silence after a few moments pedro s voice piped up ready to fly in the speakers to which i could only reply the speakers are cut that s the end of the show the moral of this story is that shakespeare profoundly understood the potential magic of the individual actor and directors with conceptual flourishes in mind should find the right time and place for them a mysterious character pops up towards the end of as you like it explaining ganymede s promise to bring about a series of extraordinary events to conclude the play orlando explains this boy is forest born and hath been tutored in the rudiments of many desperate studies by his uncle whom he reports to be a great magician obscured in the circle of this forest could this be a typically concealed and witty allusion to how shakespeare saw himself an ever present though obscured figure in the circle of the globe theatre a permanent forest of arden the hidden tutor of ganymede the forest alter ego of rosalind is also the playwright lovingly guiding his protagonist however identifying this uncle as a great magician engaged in desperate which means dangerous studies opens up another shakespearean persona and perhaps the most important some critics have located shakespeare in a mystic tradition that has ancient roots this tradition manifested itself in the movement of christian cabalism that was prevalent in italy and germany in the 16th century in england the mysterious john dee magus politician and philosopher collected an amazing library of literature devoted to mystical interpretations of the nature of the world this way of thinking was highly subversive and was inherently a secret underground movement one of its manifestations was the practice of alchemy an art steeped in astrology numerology and the power of the imagination the aim was to distill all the elements of humanity into a meaningful whole in the 20th century carl jung fused this ancient alchemical tradition with the field of psychoanalysis alchemy is intrinsic to the practice of theatre any theatrical enterprise is a mixing of different imaginations that strive to produce imaginative gold for an audience a theatre director has to distill the elements of acting design lighting sound music stage management marketing and producing to create a show the shakespeare i imagine distilled all his identities country boy town sophisticate businessman landowner father husband actor poet closet catholic etc into becoming a theatre alchemist extraordinaire someone who believed in the literal magical power of words to give shape to our human existence 10 we should always remember that shakespeare s fame was not a given his fellow playwrights beaumont fletcher were more popular ben jonson a far more celebrated writer shakespeare also showed little interest in his plays being published a new phenomenon of his times luckily his friends decided to posthumously create a complete works in 1623 in the introduction ben jonson made a crucial contribution to shakespeare s longevity praising his fellow playwright as soul of the age and for all time jonson proclaimed shakespeare as the country s greatest playwright to be set along the giants of the greek drama aeschylus euripides and sophocles this stunningly generous praise from the country s most famous writer established shakespeare in the pantheon and this published edition was widely read around the english speaking world thus marking the birth of brand shakespeare i think these phrases soul of the age and for all time still stand as a touchstone for all shakespearean productions we should always seek to understand the age that shakespeare wrote specifically as a time of change and adventurous thinking in the arts science and politics from there we must move on to the real challenge how to validate the claim that his plays endure for all time shakespeare has been a constant companion in my career staging his plays pushes your imagination to the limit they also inspire you when encountering other playwrightsi recommend any writer actor director or designer to have a relationship with the bard his shapesphereing opens up wonderful worlds for you to explore footnotes 1 the story of how brand shakespeare came about is a fascinating one inventing shakespeare by gary taylor and the genius of shakespeare by jonathan bate are two books i would recommend reading on this subject 2religion and the rise of capitalism by rh tawney religion and the decline of magic by keith thomas the family sex and marriage in england by lawrence stonechristopher hill s work and the historicist school of stephen greenblatt and others who use contemporary documents to elucidate shakespeare s plays finally james schapiro s 1599 is a wonderfully accessible weaving together of political history theatre history and shakespeare s writing 3 a particular highlight was the georgian rustavelli company s richard 111 at the roundhouse modern dress loud rock music and actors who strutted around the stage hissing their lines like a swarm of snakes and looking like they could devour each other at any movementat the centre was ramon chivadze s charismatic bullfrog of a richard a wildly incendiary piece of acting 4 the bear baiting speech 5 shakespeare describes this commercial conflict as mortal and intestine jars a brilliant phrase economic conflict costs lives and somehow is in our gut 6 juliet s speech at the start of the balcony scene act 2 scene 1 line 75 is a ringing declaration that dissolves all labels of race class and gender in the name of who one is and who one actually loves her words have an extraordinary subversive power 6a at the start of the production ian bartholemew as the fool was perched on top of the theatre staring out over the audience in the great hall the fool sung jabbered capered all over the theatre s roof as the start of the show came closer loud fanfares echoed around the hall ushering the audience into the theatre 7 this urgency is the key to unlocking the very long act 1 sc2 of the tempest prospero s account of their life to his daughter miranda is not mere exposition but a necessary and difficult immersion in the past which is a preparation for the present project dedicated to creating a new future prospero is undergoing self therapy in telling this story the ariel caliban episodes conjure up the yin yang nature of island life and prospero s strange family both relationships are at a crisis point that are about to be resolved excavating the character s past through improvisations gave a great energy to playing these scenes 8 in shakespeare s globe theatre the actor was central the power of the word paramount and scenery at a minimum this idea of theatre was threatened by the development of the court masque in the late elizabethan jacobean age court masques was a theatre of the elite the performers were drawn from court favourites including the monarch s family the budgets were massive with lavish scenic effects and gorgeous costumes through the genius of designer inigo jones theatre became a place of perspectives and detailed settings that rivalled classical paintings the court masque promoted the political orthodoxies of the establishment masques expanded outside the aristocratic court into city wide site specific spectaculars eventually the traditional proscenium arch theatre permanently enshrined the art of the masque the court masques ideology still predominates today with the theatre of the big budget technological spectacular a theatre of spectacle as opposed to substance shakespeare never became a court masque maker i think the tempest is his riposte to this style the play is full of stage magic but used to convey a profound humanistic vision of the world 9 my attitude to shakespearean language reflects my sense that he regarded words as having a literal power studying the components of shakespeare s verse is vital there is a wonderful toolbox of linguistic devices to draw upon crucially there is no one way to speak the language rp accents didn t exist in shakespeare s time regional accents predominated rp belongs to victorian notions of speaking shakespeare not elizabethan ones i think the future of shakespeare verse speaking could be very exciting the global impact of hip hop culture has resulted in the emergence of spoken word performance and the resurgence of poetry our current love of words wit rhyme and rhythm is directly connected to renaissance england we must stop actors feeling this language is not for them because it can only be spoken in one proscribed fashion for any jazz musician an understanding of tradition is important but after that the development of one s own voice is the point my funny valentine can be played a million ways and some of the jazz versions are gloriously unexpected the same should be true for to be or not to be 10 or as playwright sam shepard once put it how do pictures become words or how do words become pictures and how do they cause you to feel something that s a miracle cred